prefix bed: <http://bedretto.org/ontology#> 
prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
prefix skos: <http://www.w3.org/2004/02/skos/core#> 
prefix sh: <http://www.w3.org/ns/shacl#> 
prefix xsd: <http://www.w3.org/2001/XMLSchema#> 
prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

bed: a owl:Ontology ;
  dcterms:title "Bedretto Ontology" ;
  dcterms:author "Liliana Vargas" , "Robin Franken" ;
  dcterms:abstract "An ontology for describing boreholes, core samples, sensors, and related geological data at the Bedretto Underground Laboratory for Geosciences and Geoenergies." ;
  dcterms:description "This ontology provides classes and properties for representing boreholes, borehole logs, core samples, sensors, and related metadata for geoscientific research. It is designed to support data integration, sharing, and documentation for the Bedretto project." ;
  adms:versionNotes "Initial version. Please update with release notes as needed." ;
  dcterms:modified "2024-06-18"^^xsd:date ;
  dcterms:created "2024-05-01"^^xsd:date ;
  dcterms:creator "Bedretto Lab" ;
  dcterms:issued "2024-06-01"^^xsd:date ;
  dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
  dcterms:publisher "ETH Zurich" ;
  dcterms:rightsHolder "ETH Zurich" ;
  owl:versionInfo "0.0.2" ;
  doap:repository <https://github.com/bedretto/bedretto-ontology> ;
  rdfs:label "Bedretto Ontology" ;
  rdfs:comment "Ontology for boreholes, core samples, sensors, and related geoscientific data at Bedretto Lab." ;
.

bed:TestBed a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Test Bed" ;
  skos:definition "A test bed is a controlled environment where experiments and tests are conducted to evaluate the performance of systems or components." ;
    sh:property [sh:path bed:longName ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
        "Bedretto Underground Laboratory for Geosciences and Geoenergies (BULGG)"
        "Grimsel Test Site"
        "Mont Terri rock laboratory"
        "Geothermal research platform Groß Schönebeck"
        "Pilot site for geological storage of CO2 Ketzin"
        "Geotechnology Laboratory Fraunhofer IEG"
        "TRUDI underground laboratory"
        "Geothermal Project on TU Delft campus"
        "UK Geoenergy Observatory"
        "Cheshire Observatory"
        "Glasgow Observatory"
        "UK GeoEnergy Research Centre (GERC)"
        "Rijswijk Centre for Sustainable Geo-energy (RCSG)"
        "ECCSEL Svelvik CO2 Field Lab"
        "BRGM’s Geothermal Testing Facility"
        "Sotacarbo Fault Laboratory"
        "Underground research laboratory Andra"
        "CATenoy experimental site (CATLAB)"
        "PITOP Borehole Geophysical Test Site"
        "Krafla Magma Testbed"
    )] ; 
    sh:property [sh:path bed:shortName ; sh:datatype xsd:string ; sh:minCount 0 ; sh:maxCount 1 ; ] ; 
    sh:property [sh:path bed:type ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
        "underground research laboratory"
        "test site"
        "field-scale laboratory"
        "field-scale observatory"
    )] ;
    sh:property [sh:path bed:category ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
        "geothermal"
        "induced seismicity"
        "CO2 storage"
        "radioactive waste"
    ) ] ; 
    sh:property [sh:path bed:location ; sh:class bed:Location ; sh:minCount 1 ; sh:in ("tunnel" "borehole" "mine" "cavern" "reservoir")] ; 
.   

bed:Borehole a rdfs:Class, sh:NodeShape ;
    skos:prefLabel "Borehole" ;
    skos:definition "A borehole is the generalized term for any narrow shaft drilled in the ground" ;
  sh:property [sh:path bed:identifier ; sh:minCount 1 ; sh:in ("Uniform Resource Identifier (URI)" "Borehole Code" "International Generic Sample Number (IGSN)") ] ;
  sh:property [sh:path bed:code ; sh:minCount 1 ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:name ; sh:minCount 1 ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:generalPurpose ; sh:minCount 1 ; sh:datatype xsd:string ; sh:in ("site investigation" "decontamination" "disposal" "geotechnical" "mineral" "monitoring" "oil" "groundwater" "geothermal") ] ;
  sh:property [sh:path bed:specificPurpose; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:type ; sh:minCount 1 ; sh:datatype xsd:string ; sh:in ("extraction" "producer" "estimulation" "injector" "monitoring") ] ;
  sh:property [sh:path bed:status ; sh:minCount 1 ; sh:datatype xsd:string ; sh:in ("in use" "decommissioned" "collapsed" "abandoned") ] ;
  sh:property [sh:path bed:totalLength ; sh:datatype xsd:float ; ] ;
  sh:property [sh:path bed:lengthType; sh:minCount 1 ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:geometry; sh:minCount 1 ; sh:in ("vertical" "horizontal" "inclined") ; ] ;
  sh:property [sh:path bed:drillStyle ; sh:datatype xsd:string ; sh:in ("hammer" "cored" "rotational") ; ] ;
  sh:property [sh:path bed:drillingStart ; sh:datatype xsd:date ; ] ;
  sh:property [sh:path bed:drillEnd ; sh:datatype xsd:date ; ] ;
  sh:property [sh:path bed:boreholeDiameter ; sh:datatype xsd:float ; ] ;
  sh:property [sh:path bed:coreSectionStart ; sh:datatype xsd:float ] ;
  sh:property [sh:path bed:coreSectionEnd ; sh:datatype xsd:float ] ;
  sh:property [sh:path bed:coreSectionLength ; sh:datatype xsd:float ] ;
  sh:property [sh:path bed:drillingReport ; sh:datatype xsd:anyURI ] ;
  sh:property [sh:path bed:description; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:hasLocation ; sh:class bed:Location ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:hasBoreholeLog ; sh:class bed:BoreholeLog ; ] ;
  sh:property [sh:path bed:hasCore ; sh:class bed:Core ; ] ;
.

bed:Location a rdfs:Class, sh:NodeShape ;
    skos:prefLabel "Location" ;
    skos:definition "Precise location of a borehole's surface position defined by easting and northing coordinates." ;
    sh:property [sh:path bed:coordinateType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("Planned" "Staked out" "Surveyed") ; ] ;
    sh:property [sh:path bed:easting; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:northing; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:elevation ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:datumElevationReference ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:groundLevel ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:coordinateSystem ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("UTM (Universal Transverse Mercator)" "Geographic Coordinates (Latitude & Longitude)") ] ;
    sh:property [sh:path bed:dip ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:dipDirection ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("upward" "downward") ] ;
    sh:property [sh:path bed:azimuth ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:longitude; sh:datatype xsd:float ; ] ;
    sh:property [sh:path bed:latitude; sh:datatype xsd:float ; ] ;
    sh:property [sh:path bed:description; sh:datatype xsd:string ; ] ;
    sh:property [sh:path bed:tunnelMeter; sh:datatype xsd:int ; ] ;
    sh:property [sh:path bed:tunnelSection ; sh:datatype xsd:string ; ] ;
.
	
bed:BoreholeLog a rdfs:Class, sh:NodeShape ;
	skos:prefLabel "Borehole log" ;
	skos:definition "A systematic recording of data from a well." ;
	skos:definition "A description of a physical feature along the length of a borehole. May be continuous data (i.e. downhole geophysical) or discrete sections (i.e. geologist log)." ;
  skos:definition "Well logging, also known as borehole logging is the practice of making a detailed record (a well log) of the geologic formations penetrated by a borehole. The log may be based either on visual inspection of samples brought to the surface (geological logs) or on physical measurements made by instruments lowered into the hole(geophysical logs). Some types of geophysical well logs can be done during any phase of a well's history: drilling, completing, producing, or abandoning. Well logging is performed in boreholes drilled for the oil and gas, groundwater, mineral and geothermal exploration, as well as part of environmental and geotechnical studies." ;
  skos:example "Borehole logging; well logging; downhole log;" ;
  sh:property [sh:path bed:loggingTool ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:loggingDirection ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:loggingRun ; sh:datatype xsd:string ; ] ;
  .

bed:Core a rdfs:Class, sh:NodeShape ;
	skos:prefLabel "Core" ;
	skos:definition "A core run is defined by the drilling that happens during the time in which one core barrel is in the borehole. It starts when drilling succeeds with the empty core barrel and ends when the core barrel is full or has to be retrieved from the borehole for other reasons. The entire drill core retrieved from a borehole is the sum of drill core from all core runs in this borehole. It is also called the sampling material. IODP/ICDP naming example: 5054-1-A-178Z (Expedition number-Drill site number-Borehole character-Core run number and coring type designation)" ;
sh:property [sh:path bed:coreId ; sh:datatype xsd:string ; sh:minCount 1 ; ] ; 
sh:property [sh:path bed:originLab ; sh:datatype xsd:string ;  ] ;
sh:property [sh:path bed:campaign ; sh:datatype xsd:string ;  ] ;
sh:property [sh:path bed:coreRun ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:topDepth ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:bottomDepth ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:depthReference ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("measured depth" "vertical depth") ] ;
sh:property [sh:path bed:length ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:diameter ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:weight; ] ;
sh:property [sh:path bed:chargeNumber ; ] ;
sh:property [sh:path bed:repository ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:repositoryLocation; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:coreScan ; sh:datatype xsd:boolean ;  ] ;
sh:property [sh:path bed:method ; sh:datatype xsd:string ;  sh:in ("multi-sensor core logger (MSCL)" "X-ray fluorescence") ;] ;
sh:property [sh:path bed:hasCoreSample ; sh:class bed:CoreSample ; ] ;
sh:property [sh:path bed:hasGeology ; sh:class bed:Geology] ;
.

bed:Geology a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Geology" ;
  skos:definition "A geological description of the core, including rock classification and fabric." ;
  sh:property [sh:path bed:coreId ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:rockClassification ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:rockType ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:description ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:color ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:minerals ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:grainSize ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:texture ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:foliation ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:disturbance ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:discontinuity ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:litholog ; sh:datatype xsd:boolean ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:geologicalAge ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:geologicalUnit ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    .
  
bed:CoreSample a rdfs:Class, sh:NodeShape ;
    skos:prefLabel "Core sample" ;
    skos:definition "A core sample is a cylindrical section of rock, soil, or other material that is extracted from the ground using a drilling process. It is typically used for geological, environmental, or engineering studies to analyze the composition and properties of subsurface materials." ;
  sh:property [sh:path bed:sampleNumber ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:sampleDepth ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:samplingDate ; sh:datatype xsd:date ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:purpose ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:requesterLaboratory ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:dimensions ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:description; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:subsampleType; sh:datatype xsd:string ; sh:in ("thin section" "plug" "trim end" "powder") ; ] ;
  sh:property [sh:path bed:property ; sh:datatype xsd:string ; sh:in ("density" "porosity" "permeability" "electric conductivity" "velocity" "mineralogy" "stress" "strain") ; ] ;
  sh:property [sh:path bed:method; sh:datatype xsd:string ; sh:in ("Scaning Electron Microscopy (SEM)" "Brazillian Test" "Ultrasonic test") ] ;
  .

bed:Monitoring a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Monitoring" ;
  skos:definition "Monitoring refers to the process of systematically observing and recording data over time to assess changes or trends in a specific environment or system." ;
  sh:property [sh:path bed:monitoringType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("experimental seismic network" "background seismic network" "distributed fibre optic sensing") ] ;
  sh:property [sh:path bed:monitoringCategory ; sh:datatype xsd:date ; sh:in ("downhole monitoring" "surface monitoring" "tunnel monitoring") ] ;
  .

bed:DownholeLogging a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Downhole Logging" ;
  skos:definition "Downhole logging is the process of measuring and recording physical properties of the borehole and surrounding rock formations using specialized instruments lowered into the borehole." ;
  sh:property [sh:path bed:boreholeCode ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:loggingDate ; sh:datatype xsd:date ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:logType ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:operatorName ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:institution ; sh:datatype xsd:string ; ] ;
  sh:property [sh:path bed:tool ; sh:datatype xsd:string ; sh:in (
                                                                    "Acoustic televiewer (ATV)"
                                                                    "Optic televiewer (OTV)"
                                                                    "Deviation Tool (DEV)"
                                                                    "Spectral Gamma (SGR)"
                                                                    "Fluid Temperature and Conductivity (FTC)"
                                                                    "Caliper (CAL)"
                                                                    "Full Waveform Sonic (FWS)"
                                                                    "Magnetic susceptibility OSG"
                                                                    "Mudparameter OSG"
                                                                    "Sidewall density"
                                                                    "Temperature"
                                                                    "Conductivity"
                                                                    "Three-arm caliper"
                                                                ) ] ;
  sh:property [sh:path bed:loggingDirection ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("upward" "downward") ] ;
  sh:property [sh:path bed:loggingRun ; sh:datatype xsd:int ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:centralizedTool ; sh:datatype xsd:boolean ; sh:minCount 1 ; ] ;
  sh:property [sh:path bed:samplingRate ; sh:datatype xsd:float ; ] ;
  sh:property [sh:path bed:samplingRateUnit ; sh:datatype xsd:string ; sh:in ("meter" "inch" "second")] ;
  sh:property [sh:path bed:magneticInclination ; sh:datatype xsd:float ; ] ;
  sh:property [sh:path bed:magneticDeclination ; sh:datatype xsd:float ; ] ;
  sh:property [sh:path bed:loggingReport ; sh:datatype xsd:boolean ; ] ;
  sh:property [sh:path bed:compositeLog ; sh:datatype xsd:boolean ; ] ;
  .


bed:Sensor a rdfs:Class, sh:NodeShape ;
	skos:prefLabel "Sensor" ;
	skos:definition "Mechanical or electronic devices for measuring various properties in the well. These devices can be permanent; in this case, they are left in place for months or even years." ; 
    sh:property [sh:path bed:sensorID ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:identifier ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:sensorGroup ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("Seismic sensor" 
                                                                                           "hydromechanical sensor" 
                                                                                           "hydraulic sensor" 
                                                                                           "fibre optic") ] ;
    sh:property [sh:path bed:sensorType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
                                                                                        "seismometer"
                                                                                        "geophone"
                                                                                        "accelerometer"
                                                                                        "acoustic emission sensor"
                                                                                        "strainmeter"
                                                                                        "tiltmeter"
                                                                                        "piezometer"
                                                                                        "flowmeter"
                                                                                    ) ] ;
    sh:property [sh:path bed:sensorSubtype ; sh:datatype xsd:string ; ] ;
    sh:property [sh:path bed:serialNumber ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:model ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:manufacturer ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
    sh:property [sh:path bed:deploymentType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("permanent" "temporary") ] ;
    sh:property [sh:path bed:sensorStatus ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("reserved" "damaged" "ceased")] ;
  .

bed:Configuration a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Configuration" ;
  skos:definition "A configuration is a set of parameters that define the operation of a sensor or system." ;
sh:property [sh:path bed:boreholeId ; sh:datatype xsd:int ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:sensorID ; sh:datatype xsd:int ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:sensorCode ; sh:datatype xsd:string ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:sensorStatus ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in ("planned" "installed" "removed") ] ;
sh:property [sh:path bed:installationDate ; sh:datatype xsd:date ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:removalDate ; sh:datatype xsd:date ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:description ; sh:datatype xsd:string ; ] ;
sh:property [sh:path bed:locationId ; sh:datatype xsd:int ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:sensorDepth ; sh:datatype xsd:float ; sh:minCount 1 ; ] ;
sh:property [sh:path bed:orientation ; sh:datatype xsd:string ; ] ;
sh:property [sh:path bed:deploymentType ; sh:datatype xsd:string ; sh:in ("cemented" "clamped")] ;
.

bed:SeismicSensor a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Seismic Sensor" ;
  skos:definition "A seismic sensor is a device used to detect and measure seismic waves, typically used in geophysical studies." ;
  sh:property [sh:path bed:sensorType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
                                                                                        "AE GMuG-Ma-Blc-30-60 (C )"
                                                                                        "AE GMuG-Ma-Blw-7-140 (SV-SpW)"
                                                                                        "AE GMuG-Ma-Blw-7-70 (SV-SpW)"
                                                                                        "AE Transmitter - Blw-Tr40 (SV-SpW)"
                                                                                        "AE Transmitter - Blw-Tr50 (SV-SpW)"
                                                                                        "Accelerometer PCB 3c (<3kHz)"
                                                                                        "Accelerometer Wilcoxon 1c (<25kHz)"
                                                                                        "Cascadia posthole"
                                                                                        "IMS Geophone (100 Hz)"
                                                                                        "MBAS Geophone (100 Hz)"
                                                                                        "Titan posthole"
                                                                                        "Trillium Posthole"
                                                                                        "EpiSensor"
                                                                                    ) ] ;
  .

bed:HydromechanicalSensor a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Hydromechanical Sensor" ;
  skos:definition "A hydromechanical sensor is a device used to measure hydromechanical properties such as pressure, flow, and temperature in boreholes." ;
  sh:property [sh:path bed:sensorType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
                                                                                        "CTPP / Keller pressure probe"
                                                                                        "step-rate injection method for fracture in-situ properties (SIMFIP)"
                                                                                        "DORSA"
                                                                                        "Tiltmetre CSIRO"
                                                                                    ) ] ;
  .

bed:HydraulicSensor a rdfs:Class, sh:NodeShape ;
  skos:prefLabel "Hydraulic Sensor" ;
  skos:definition "A hydraulic sensor is a device used to measure hydraulic properties such as pressure and flow in boreholes." ;
  sh:property [sh:path bed:sensorType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
                                                                                        "IMI (?) Packer"
                                                                                        "multi-packer"
                                                                                        "flowmeter"
                                                                                    ) ] ;
  .

bed:FibreOpticSensor a rdfs:Class, sh:NodeShape ;
    skos:prefLabel "Fibre Optic Sensor" ;
    skos:definition "A fibre optic sensor is a device that uses optical fibers to measure various physical properties such as temperature, strain, and pressure." ;
    sh:property [sh:path bed:sensorType ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
                                                                                            "distributed acoustic sensing (DAS)"
                                                                                            "distributed strain sensing (DSS)"
                                                                                            "BRUsens strain sensing cable"
                                                                                            "distributed temperature sensing (DTS)"
                                                                                            "fibre bragg grating sensor (FBG)"
                                                                                        ) ] ;
    sh:property [sh:path bed:interrogator ; sh:datatype xsd:string ; sh:minCount 1 ; sh:in (
                                                                                            "FBG interrogator"
                                                                                            "optoelectronics interrogator"
                                                                                            "Silixa DAS interrogator #(iDAS) #XT-DTS"
                                                                                            "HYPERION Optical Sensing Instrument"
                                                                                        ) ] ;
    .
    
bed:identifier a rdf:Property ;
  skos:prefLabel "identifier" ;
  skos:definition "URI, code, IGSN, unique identifier"
  .

bed:generalPurpose a rdf:Property ;
  skos:prefLabel "General Purpose" ;
  skos:definition "Purposes for which a borehole was drilled" ;
  skos:example "site investigation, decontamination, disposal, Geotechnical, Mineral, MonitoringlevelHead, MonitoringQuality, Oil, OilExploratory, Seismic, WaterExploratory, etc." ;
  .
bed:type a rdf:Property ;
  skos:prefLabel "Type" ;
  skos:definition "Specific purpose of the borehole" ;
  skos:example "extraction, production, injection, estimulation, observation, monitoring" ;
  .
bed:status a rdf:Property ;
  skos:prefLabel "Status" ;
  skos:definition "Current status of the borehole" ;
  skos:example "in use, functioning, decommissioned, collapsed, abandoned" ;
  .
bed:totalLength a rdf:Property ;
  skos:prefLabel "Total Length" ;
  skos:definition "The distance along a borehole." ;
  skos:example "This will be determined by the data provider (ie, 'length' can have different sources, like drillers measurement, loggers measurement, survey)" ;
  .
bed:coordinateSystem a rdf:Property ;
  skos:prefLabel "Coordinate System" ;
  skos:definition "Spatial reference system".

bed:elevation a rdf:Property ;
  skos:prefLabel "Elevation" ;
  skos:definition "The vertical height above datum of the borehole collar.".

bed:elevationReference a rdf:Property ;
  skos:prefLabel "Elevation Reference" ;
  skos:definition "also known as datum. It is an absolute reference such as the sea level or ground level".

bed:datumElevationReference a rdf:Property ;
  skos:prefLabel "Ground level relative to datum" ;
  skos:definition "This is the distance from the sea level to the elevation of the location where the borehole is located"
  .

bed:dip a rdf:Property ;
  skos:prefLabel "Borehole Dip" ;
  skos:definition "Also Borehole inclination. " ;
  skos:example "Upward, downward" ;
  .
bed:azimuth a rdf:Property ;
  skos:prefLabel "azimuth " ;
  skos:definition ""
  .
bed:drillStyle a rdf:Property ;
  skos:prefLabel "Drill style" ;
  skos:definition "method used to drill hole" ;
